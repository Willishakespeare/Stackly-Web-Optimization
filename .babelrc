{
  "presets": ["next/babel"],
  "env": {
    "production": {
      "plugins": [
        ["@emotion"],
        ["@babel/plugin-transform-arrow-functions"],
        ["minify-dead-code-elimination"],
        ["minify-simplify"],
        ["babel-plugin-transform-remove-undefined"]
      ]
    },
    "development": {
      "plugins": [["@emotion", { "sourceMap": true }]]
    },
    "test": {
      "plugins": [
        [
          "@emotion",
          { "sourceMap": true, "autoLabel": true, "labelFormat": "âš¡[local]" }
        ]
      ]
    }
  }
}
